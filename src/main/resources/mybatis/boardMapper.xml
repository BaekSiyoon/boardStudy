<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="com.example.board.board.mapper.boardMapper">

  <select id="list" resultType="boardVo">
    SELECT 
      seq, title, content, regId, regDate
    FROM 
      boardList
    ORDER BY 
      regDate DESC LIMIT 0,5;
  </select>

  <insert id="insert"> 
    INSERT INTO 
      boardList(title ,content ,password ,regId)
    VALUES 
      (#{title}, #{content}, #{password}, #{regId});
  </insert>

  <select id="detailList" resultType="boardVo">
    SELECT
      seq, title, content, regId, regDate
    FROM
      boardList
    WHERE
      seq = #{seq};
  </select>

  

 </mapper>